{
    "name": "cat-maker",
    "version": "1.0.0",
    "description": "Design your custom cats in whatever way you want.",
    "apiBase": "/api/v1/",
    "author": "William McGonagle",
    "keywords": [
        "cats",
        " fun",
        " cute"
    ],
    "sequelize": {},
    "objects": [
        {
            "name": "User",
            "path": "user",
            "model": {
                "username": "string",
                "password": "text",
                "description": "text",
                "email": "text",
                "birthday": "date",
                "emailVerified": "boolean"
            },
            "paths": [
                {
                    "path": "/signup",
                    "method": "post",
                    "type": "SIGNUP",
                    "params": []
                },
                {
                    "path": "/login",
                    "method": "post",
                    "type": "LOGIN",
                    "params": []
                },
                {
                    "path": "/:id/",
                    "method": "get",
                    "type": "GET",
                    "params": []
                }
            ]
        }
    ]
}
